{
  "name": "@djs-core/builder",
  "version": "1.0.0",
  "description": "Builder for djs-core",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Cleboost",
  "repository": {
    "url": "https://github.com/djs-core/djs-builder"
  },
  "bugs": {
    "url": "https://github.com/djs-core/djs-builder/issues"
  },
  "scripts": {
    "dev": "tsup --config tsup-dev.config.ts",
    "build": "tsup --config tsup-prod.config.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "test": "npm run build",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "glob": "11.0.1",
    "javascript-obfuscator": "4.1.1",
    "tsup": "8.3.6"
  },
  "devDependencies": {
    "@eslint/js": "9.19.0",
    "@types/node": "22.12.0",
    "eslint": "9.19.0",
    "eslint-plugin-headers": "1.2.1",
    "globals": "15.14.0",
    "prettier": "3.4.2",
    "typescript": "5.7.3",
    "typescript-eslint": "8.22.0"
  },
  "engines": {
    "node": ">=16.6.0"
  }
}
